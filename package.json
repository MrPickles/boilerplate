{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "Experimental boilerplate code.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run prod",
    "dev": "NODE_ENV=development nodemon index.js",
    "prod": "NODE_ENV=production node index.js",
    "build": "npm run copy && NODE_ENV=production webpack -p",
    "copy": "copyfiles public/img/* dist",
    "webpack": "npm run copy && NODE_ENV=development webpack-dev-server",
    "clean": "rm -rf ./dist/",
    "test": "NODE_ENV=test mocha",
    "coverage": "istanbul cover _mocha -- -R spec",
    "lint": "eslint --ext .js --ext .jsx index.js .storybook app server",
    "heroku-postbuild": "npm run build &",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MrPickles/boilerplate.git"
  },
  "author": "Andrew Liu",
  "dependencies": {
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "mongoose": "^4.7.6",
    "morgan": "^1.8.1",
    "prop-types": "^15.5.4",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.3.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^2.8.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "react-test-renderer": "^15.5.4",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.5.4",
    "write-file-webpack-plugin": "^4.0.2"
  }
}
